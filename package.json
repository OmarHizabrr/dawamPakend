{
    "private": true,
    "scripts": {
      "build": "yarn production",
      "start": "yarn hot",
      "dev": "yarn development",
      "development": "cross-env NODE_ENV=development webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
      "watch": "yarn development --watch",
      "watch-poll": "yarn watch --watch-poll",
      "hot": "cross-env NODE_ENV=development webpack serve --hot --client-web-socket-url ws://localhost:8081/ws --allowed-hosts all --config=node_modules/laravel-mix/setup/webpack.config.js",
      "prod": "yarn production",
      "production": "cross-env NODE_ENV=production webpack --no-progress --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
      "@babel/core": "^7.24.5",
      "@babel/preset-env": "^7.24.5",
      "axios": "^0.19",
      "cross-env": "^7.0",
      "laravel-mix": "^6.0.49",
      "lodash": "^4.17.19",
      "postcss": "^8.4.38",
      "resolve-url-loader": "^3.1.0",
      "vue-template-compiler": "^2.7.16",
      "webpack": "^5.89.0",
      "webpack-cli": "^4.10.0",
      "webpack-dev-server": "^4.15.1"
    }
  }
